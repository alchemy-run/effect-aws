{
  "name": "effect-aws",
  "module": "index.ts",
  "version": "0.0.1",
  "type": "module",
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false,
  "files": [
    "src",
    "lib"
  ],
  "scripts": {
    "setup": "bun run scripts/setup.ts",
    "generate": "bun run scripts/generate-clients.ts",
    "clean": "git clean -fqdx -e .env",
    "clean:install": "bun run clean && bun install",
    "clean:build": "bun run clean:install && bun run build",
    "build": "bun generate && bun oxfmt ./src/services/* && tsgo -b",
    "test:local": "LOCAL=1 bun run scripts/test.ts",
    "test:live": "bun run scripts/test.ts",
    "test:protocols": "bun vitest run ./test/protocols/",
    "test:services": "bun test:local ./test/services/",
    "test:services:live": "bun test:live ./test/services/",
    "format": "bun oxfmt ./scripts ./src ./test",
    "find:errors": "bun run scripts/find-errors.ts",
    "symlink": "bun run scripts/symlink.ts"
  },
  "exports": {
    "./*": {
      "types": "./lib/services/*.d.ts",
      "bun": "./src/services/*.ts",
      "default": "./lib/services/*.js"
    },
    ".": {
      "types": "./lib/index.d.ts",
      "bun": "./src/index.ts",
      "default": "./lib/index.js"
    }
  },
  "peerDependencies": {
    "effect": "latest",
    "@effect/platform": "latest"
  },
  "dependencies": {
    "@aws-crypto/crc32": "^5.2.0",
    "@aws-sdk/credential-providers": "^3.933.0",
    "@smithy/util-base64": "^4.3.0",
    "aws4fetch": "^1.0.20",
    "fast-xml-parser": "^5.3.2"
  },
  "devDependencies": {
    "@effect/ai": "^0.33.2",
    "@effect/ai-anthropic": "^0.23.0",
    "@effect/ai-openai": "^0.37.1",
    "@effect/cli": "^0.73.0",
    "@effect/cluster": "latest",
    "@effect/experimental": "latest",
    "@effect/platform-bun": "latest",
    "@effect/platform-node": "latest",
    "@effect/rpc": "latest",
    "@effect/sql": "latest",
    "@effect/vitest": "latest",
    "@effect/workflow": "latest",
    "@rsbuild/core": "^1.6.7",
    "@types/bun": "latest",
    "@types/node": "^24.10.1",
    "@typescript/native-preview": "^7.0.0-dev.20251230.1",
    "dedent": "^1.7.0",
    "effect": "latest",
    "oxfmt": "0.21.0",
    "oxlint": "1.36.0",
    "pathe": "^2.0.3",
    "typescript": "^5"
  }
}